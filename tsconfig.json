{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "outDir": "./dist",
    "incremental": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true
  }
}
